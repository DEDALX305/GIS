			            button.removeClass('active');
			            box.hide();
			        }
			    });
			// Login Form End
				$('div.input-mod').click(function() {
					var div = $(this);
					var inp = $('#'+div.attr('for'));
					var val = (div.attr('id')=='input-minus'?-1:1);
					if (inp) {
						var step=(inp.attr('step')?inp.attr('step'):1);
						if ($.isNumeric(inp.val())) {
							val=parseInt(inp.val())+val*step;
						} else {
							val=val*step;
						}
						var min=(inp.attr('min')?inp.attr('min'):0);
						if ((!inp.attr('max') || val<=inp.attr('max')) && val>=min) {
							if (val==0 && inp.attr('zero')) val=inp.attr('zero');
							inp.val(val);
							inp.change();
						}
					}
				});

				$("div.primary-front-menu").find("a[href='" + top.location.pathname + "']").each(function (){
