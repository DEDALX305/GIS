			}
			function showPin(id){
					var p=$("a#"+id);
					margin=parseInt(p.parent().css('marginLeft'));
					$("div#menu-pin").css({'left': p.position().left-margin-6+p.innerWidth()/2, 'top': $("div.main-menu").innerHeight()-6});
					$("div#menu-pin").show();
